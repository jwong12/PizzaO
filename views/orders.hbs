<link rel="stylesheet" href="stylesheets/orders.css">
<div id="container">
    <h2>{{title}}</h2><br/>
    <form method="post" id="search">        
        <label for="fname">First name: </label><input type="text" name="fname" id="fname" placeholder="James">
        <label for="lname">Last name: </label><input type="text" name="lname" id="lname" placeholder="Wong">
        <label for="phone">Phone number: </label><input type="text" name="phone" id="phone" placeholder="6041235555">
        <button type="submit">Search</button>
    </form><br/>
    <table class="table" id="orders">
    </table>
</div>
<script src="javascripts/orders.js"></script>